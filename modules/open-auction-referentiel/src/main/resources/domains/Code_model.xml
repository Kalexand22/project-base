<?xml version="1.0"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">
  <module name="openauction" package="com.axelor.apps.openauction.db"/>


  <entity name="Epoque">
    <string name="code" title="Code"/>
    <string name="description" title="Description" namecolumn="true"/>
  </entity>
  <entity name="Siecle">
    <string name="code" title="Code"/>
    <string name="description" title="Description" namecolumn="true"/>
  </entity>
  <entity name="Ecole">
    <string name="type" title="Type" selection="openauction.schoolType.select"/>
    <string name="code" title="Code"/>
    <string name="description" title="Description" namecolumn="true"/>
  </entity>
  <entity name="Style">
    <string name="code" title="Code"/>
    <string name="description" title="Description" namecolumn="true"/>
  </entity>
  <entity name="HistoricalPeriod">
    <string name="code" title="Code"/>
    <string name="description" title="Description" namecolumn="true"/>
  </entity>

  <entity name="InventoryCategoryProduct">
    <string name="code" title="Code"/>
    <string name="description" title="Description" namecolumn="true"/>
  </entity>

  <entity name="InventoryType">
    <string name="code" title="Code"/>
    <string name="description" title="Description" namecolumn="true"/>
  </entity>

  <entity name="LotCondition">
    <string name="code" title="Code"/>
    <string name="description" title="Description" namecolumn="true"/>
  </entity>

  <entity name="PassingIndex">
    <string name="code" title="Code"/>
    <string name="description" title="Description" namecolumn="true"/>
  </entity>

  <entity name="RetirementReason">
    <string name="code" title="Code"/>
    <string name="description" title="Description" namecolumn="true"/>
  </entity>
</domain-models>