      //openauction.contactImputationType.select
      public static final String CONTACTIMPUTATIONTYPE_SELECT_SELLER = "Seller0";
      public static final String CONTACTIMPUTATIONTYPE_SELECT_BUYER = "Buyer1";
      public static final String CONTACTIMPUTATIONTYPE_SELECT_NONE = "None2";


      //openauction.serviceType.select.missionserviceline
      public static final String SERVICETYPE_SELECT_COMMISSION = "Commission0";
      public static final String SERVICETYPE_SELECT_EXPENSE = "Expense1";
      public static final String SERVICETYPE_SELECT_AUCTIONBID = "AuctionBid2";


      //openauction.type.select.servicetemplateline
      public static final String TYPE_SELECT_SERVICETEMPLATELINE_SERVICE = "Service0";
      public static final String TYPE_SELECT_SERVICETEMPLATELINE_RESSOURCE = "Ressource1";
      public static final String TYPE_SELECT_SERVICETEMPLATELINE_EMPTY = "Empty2";
      ]]>
    </extra-code>
  </entity>


</domain-models>
